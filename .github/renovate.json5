{
  extends: [
    'github>camptocamp/gs-renovate-config-preset:base.json5#1.4.2',
    'github>camptocamp/gs-renovate-config-preset:group.json5#1.4.2',
    'github>camptocamp/gs-renovate-config-preset:ci.json5#1.4.2',
    'github>camptocamp/gs-renovate-config-preset:preset.json5#1.4.2',
    'github>camptocamp/gs-renovate-config-preset:pre-commit.json5#1.4.2',
    'github>camptocamp/gs-renovate-config-preset:docker.json5#1.4.2',
    'github>camptocamp/gs-renovate-config-preset:json-schema.json5#1.4.2',
    'github>camptocamp/gs-renovate-config-preset:shellcheck.json5#1.4.2',
    'github>camptocamp/gs-renovate-config-preset:stabilization-branches.json5#1.4.2',
    'github>camptocamp/gs-renovate-config-preset:own.json5#1.4.2',
  ],
  baseBranchPatterns: ['master', '4.4.0'],
  packageRules: [
    /** Docker images versioning */
    {
      matchDatasources: ['docker'],
      versioning: 'loose',
    },
    /** PostGis versioning */
    {
      matchDatasources: ['docker'],
      matchDepNames: ['postgis/postgis'],
      versioning: 'regex:^(?<major>\\d+)-(?<minor>\\d+)\\.(?<patch>\\d+)-(?<compatibility>.*)$',
    },
    {
      matchFiles: ['acceptance_tests/Dockerfile'],
      matchDatasources: ['docker'],
      versioning: 'ubuntu',
    },
    /** Parse Postgres version version like 12-3.4-alpine */
    {
      matchDatasources: ['docker'],
      matchPackageNames: ['postgres/postgres'],
      versioning: 'regex:^(?<major>\\d+)-(?<minor>\\d+)\\.(?<patch>\\d+)-(?<compatibility>.+)$',
    },
    {
      matchBaseBranches: ['/^[0-9]+\\.[0-9]+\\.[0-9]+$/'],
      matchUpdateTypes: ['major', 'minor', 'pin', 'digest', 'lockFileMaintenance', 'rollback', 'bump'],
      enabled: false,
    },
    /** Disable upgrading the supported Python version */
    {
      matchFileNames: ['acceptance_tests/pyproject.toml'],
      matchPackageNames: ['python'],
      enabled: false,
    },
    /** Packages published very recently are not pushed to stabilization branches for security reasons */
    {
      matchBaseBranches: ['/^[0-9]+\\.[0-9]+\\.[0-9]+$/'],
      minimumReleaseAge: '7 days',
    },
  ],
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
}
